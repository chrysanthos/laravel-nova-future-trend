{
    "name": "chrysanthos/laravel-nova-future-trend",
    "description": "A Laravel Nova Trend for Future Graphs",
    "keywords": [
        "chrysanthos",
        "Laravel Nova",
        "Laravel Nova Future Trend"
    ],
    "homepage": "https://github.com/chrysanthos/laravel-nova-future-trend",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chrysanthos Prodromou",
            "email": "me@chrysanthos.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/nova": "^4.24"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chrysanthos\\LaravelNovaFutureTrend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrysanthos\\LaravelNovaFutureTrend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chrysanthos\\LaravelNovaFutureTrend\\LaravelNovaFutureTrendServiceProvider"
            ],
            "aliases": {
                "LaravelNovaFutureTrend": "Chrysanthos\\LaravelNovaFutureTrend\\LaravelNovaFutureTrendFacade"
            }
        }
    }
}
